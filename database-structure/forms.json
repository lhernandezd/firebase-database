            "contentType": "address",
            "fieldType": "TextField",
            "condition": {
              "id": "04-03-paymentMethod",
              "value": "en efectivo"
            }
          },
          "03": {
            "label": "Consignación bancaria",
            "description": "Diligencia los siguientes datos de la cuenta bancaria a la cual se consignará el pago",
            "required": "true",
            "icon": "MoneyIcon",
            "idField": "05-03-paymentMethod",
            "contentType": "paymentMethod",
            "fieldType": "paymentMethodComponent",
            "componentFields": {
              "01": {
                "label": "Tipo de cuenta",
                "description": "Señala el tipo de cuenta",
                "idField": "05-03-01-account",
                "contentType": "account",
                "fieldType": "Picker"
              },
              "02": {
                "label": "Banco",
                "description": "Señala el nombre del banco",
                "idField": "05-03-02-additional",
                "contentType": "additional",
                "fieldType": "TextField"
              }
            },
            "condition": {
              "id": "04-03-paymentMethod",
              "value": "consignacion bancaria"
            }
          },
          "04": {
            "label": "Número de cuenta",
            "description": "Señala el número de cuenta",
            "required": "true",
            "icon": "MoneyIcon",
            "idField": "05-04-number",
            "contentType": "number",
            "fieldType": "TextField",
            "condition": {
              "id": "04-03-paymentMethod",
              "value": "consignacion bancaria"
            }
          },
          "05": {
            "label": "Titular de la cuenta ",
            "description": "Señala el nombre del titular de la cuenta",
            "required": "true",
            "icon": "NameIcon",
            "idField": "05-05-additional",
            "contentType": "additional",
            "fieldType": "TextField",
            "condition": {
              "id": "04-03-paymentMethod",
              "value": "consignacion bancaria"
            }
          },
          "06": {
            "label": "Gastos del traspaso",
            "description": "Señala quien correrá con los gastos del traspaso, o si se dividirá en partes iguales.",
