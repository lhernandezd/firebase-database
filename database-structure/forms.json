            "icon": "DateIcon",
            "idField": "06-01-date",
            "contentType": "date",
            "fieldType": "DatePicker"
          },
          "02": {
            "label": "Responsabilidad",
            "description": "Selecciona la fecha a partir de la cual el comprador es o será responsable del vehículo. Es importante este punto en los casos en que la entrega material del bien se hizo antes del traspaso ",
            "idField": "06-02-date",
            "required": "true",
            "icon": "ReasonsIcon",
            "contentType": "date",
            "fieldType": "DatePicker"
          },
          "03": {
            "label": "Fecha de firma ",
            "description": "Señala la fecha en la cual será firmado el contrato de compraventa",
            "required": "true",
            "icon": "DateIcon",
            "idField": "06-03-date",
            "contentType": "date",
            "fieldType": "DatePicker"
          },
          "04": {
            "label": "Ciudad",
            "description": "Señala la ciudad en la cual se firmará el contrato de compraventa",
            "required": "true",
            "icon": "AddressIcon",
            "idField": "06-04-destinationCity",
            "contentType": "destinationCity",
            "fieldType": "TextField"
          },
          "05": {
            "label": "Ejemplares",
            "description": "Señala el número de ejemplares del contrato que serán impresos",
            "required": "true",
            "icon": "ReasonsIcon",
            "idField": "06-05-number",
            "contentType": "number",
            "fieldType": "TextField"
          }
        }
      }
